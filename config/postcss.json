{
  "output": "./dist/css/app.css",
  "input": "./src/css/app.css",
  "use": [
    "postcss-import",
    "postcss-mixins",
    "postcss-size",
    "postcss-nested",
    "postcss-simple-vars",
    "autoprefixer",
    "postcss-clearfix",
    "postcss-color-function",
    "postcss-calc",
    "postcss-custom-media",
    "css-mqpacker",
    "csswring"
  ],
  "autoprefixer": {
    "browsers": [
      "ie 9",
      "ie 11",
      "last 1 Edge versions",
      "last 1 Firefox versions",
      "last 1 Chrome versions",
      "last 1 Safari versions",
      "iOS >= 7",
      "Android >= 4.1",
      "last 1 ChromeAndroid versions"
    ]
  }
}
